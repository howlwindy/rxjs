{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm up --latest && vite serve --mode development",
    "test": "",
    "test:unit": "jest",
    "test:e2e": "",
    "prod": "vite serve --mode production",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "eslint": "eslint . --ext .ts,.vue --fix",
    "stylelint": "stylelint \"**/*.{css,vue}\" --color --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{html,json,yaml,md,ts,css,vue}": [
      "prettier . --write"
    ],
    "**/*.{ts,vue}": [
      "eslint . --ext .ts,.vue --fix"
    ],
    "**/*.{css,vue}": [
      "stylelint \"**/*.{css,vue}\" --color --fix"
    ]
  },
  "dependencies": {
    "@vueuse/components": "^8.7.5",
    "@vueuse/core": "^8.7.5",
    "@vueuse/integrations": "^8.7.5",
    "@vueuse/motion": "2.0.0-beta.12",
    "highlight.js": "^11.5.1",
    "pinia": "^2.0.14",
    "rxjs": "^7.5.5",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-import": "^14.1.0",
    "postcss-reporter": "^7.0.5",
    "postcss-sorting": "^7.0.1",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vue-eslint-parser": "^9.0.3",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^0.38.2"
  }
}
